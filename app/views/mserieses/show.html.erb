<%= link_to "#{@series.title}<br>#{image_tag(@series.image_size_uncanny, alt: @series.title)}".html_safe, @series.url %><br><br>

<%= link_to "Link to Series on Marvel.com", @series.url %><br><br>

<%= form_tag mseries_retrieve_path, method: "get" do %>
  <%= hidden_field_tag :id, @series.id %>
  <%= submit_tag("find comics") %>
<% end %>

<%= render partial: "mcomics/comic", collection: @series.mcomics.sort_by { |c| c.issueNumber} %>